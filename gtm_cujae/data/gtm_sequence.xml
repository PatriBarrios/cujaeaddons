<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Secuencia para los gastos de viaje -->
        <record id="seq_travel_expense" model="ir.sequence">
            <field name="name">Travel Expense Reference</field>
            <field name="code">travel.expense</field>
            <field name="prefix">PC-VIAJE/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
    <data noupdate="1">
        <!-- Secuencia para formularios de viaje -->
        <record id="seq_travel_form" model="ir.sequence">
            <field name="name">Secuencia de Formularios de Viaje</field>
            <field name="code">travel.form</field>
            <field name="prefix">F-VIAJE-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>