<odoo>
    <data noupdate="1">
        <record id="event_stage_draft" model="event.stage">
            <field name="name">Borrador</field>
            <field name="sequence">1</field>
            <field name="fold" eval="False"/>
        </record>

        <record id="event_stage_published" model="event.stage">
            <field name="name">Publicado</field>
            <field name="sequence">2</field>
            <field name="fold" eval="False"/>
        </record>

        <record id="event_stage_in_progress" model="event.stage">
            <field name="name">En Transcurso</field>
            <field name="sequence">3</field>
            <field name="fold" eval="True"/>
        </record>

        <record id="event_stage_finalized" model="event.stage">
            <field name="name">Finalizado</field>
            <field name="sequence">4</field>
            <field name="fold" eval="True"/>
        </record>

        <record id="event_stage_canceled" model="event.stage">
            <field name="name">Cancelado</field>
            <field name="sequence">5</field>
            <field name="fold" eval="True"/>
        </record>
    </data>
</odoo>
